{
  "name": "realsy-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production babel-node ./server.js",
    "start:dev": "npm run build:admin && nodemon --legacy-watch --exec babel-node ./server.js",
    "prestart:test-env": "rm -f ./.tmp/test.db",
    "start:test-env": "NODE_ENV=test ENV_PATH=__tests__/.env.test babel-node ./server.js",
    "poststart:test-env": "rm -f ./.tmp/test.db",
    "build": "NODE_ENV=production npm run build:admin",
    "build:admin": "babel-node node_modules/strapi/bin/strapi.js build",
    "strapi": "strapi",
    "jest": "jest",
    "test": "jest --detectOpenHandles",
    "test:unit": "jest --group=unit --detectOpenHandles",
    "test:integration": "jest --group=integration --detectOpenHandles",
    "test:e2e": "jest --group=e2e --detectOpenHandles"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@jest/globals": "^26.6.2",
    "@shopify/jest-koa-mocks": "^2.3.0",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.19",
    "@types/koa": "^2.11.6",
    "@types/supertest": "^2.0.10",
    "babel-jest": "^25.1.0",
    "faker": "^5.1.0",
    "jest": "^25.1.0",
    "jest-runner-groups": "^1.1.1",
    "nodemon": "^2.0.2",
    "supertest": "^6.0.1"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@ckeditor/ckeditor5-build-classic": "^16.0.0",
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "@sentry/node": "^5.29.0",
    "@sentry/tracing": "^5.29.0",
    "axios": "^0.21.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "docusign-esign": "^5.2.0",
    "dotenv": "^8.2.0",
    "ellipsize": "^0.1.0",
    "email-validator": "^2.0.4",
    "fast-xml-parser": "^3.16.0",
    "file-type": "^14.1.4",
    "knex": "<0.20.0",
    "koa-body": "^4.1.1",
    "lodash": "^4.17.15",
    "nexmo": "^2.6.0",
    "ngrok": "^3.2.7",
    "pg": "^7.18.2",
    "pino": "^5.17.0",
    "pino-loggly": "^2.0.1",
    "pino-pretty": "^4.3.0",
    "qs": "^6.9.4",
    "source-map-support": "^0.5.19",
    "sqlite3": "latest",
    "strapi": "3.4.1",
    "strapi-admin": "3.4.1",
    "strapi-connector-bookshelf": "3.4.1",
    "strapi-plugin-content-manager": "3.4.1",
    "strapi-plugin-content-type-builder": "3.4.1",
    "strapi-plugin-email": "3.4.1",
    "strapi-plugin-graphql": "3.4.1",
    "strapi-plugin-upload": "3.4.1",
    "strapi-plugin-users-permissions": "3.4.1",
    "strapi-provider-email-amazon-ses": "^3.0.0",
    "strapi-provider-upload-aws-s3": "^3.2.5",
    "strapi-utils": "3.4.1",
    "tmp-promise": "^2.0.2"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  }
}
